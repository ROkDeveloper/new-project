	               |			И (НДСЗаписиКнигиПродаж.Покупатель = ДБГруппыКонтрагентовДляОплатЖКХАВАСрезПоследних.Контрагенты)
	               |ГДЕ
	               |	НДСЗаписиКнигиПродаж.Регистратор ССЫЛКА Документ.РеализацияТоваровУслуг
	               |	И РеализацияУслугОбороты.Организация = &Организация
	               |	И ДБГруппыКонтрагентовДляОплатЖКХАВАСрезПоследних.ГруппыОтчетности <> ЗНАЧЕНИЕ(Перечисление.ДБГруппыОтчетности.ПрочиеПотребители)
	               |	И ДБГруппыКонтрагентовДляОплатЖКХАВАСрезПоследних.ГруппыОтчетности <> ЗНАЧЕНИЕ(Перечисление.ДБГруппыОтчетности.Население)
	               |	И ДБГруппыКонтрагентовДляОплатЖКХАВАСрезПоследних.ГруппыОтчетности <> ЗНАЧЕНИЕ(Перечисление.ДБГруппыОтчетности.Бюджет)
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	Итог.Организация КАК Организация,
	               |	Итог.Услуга КАК Услуга,
	               |	СУММА(ЕСТЬNULL(Итог.Итог6, 0)) КАК Итог6,
	               |	СУММА(ЕСТЬNULL(Итог.Итог8, 0)) КАК Итог8,
	               |	СУММА(ЕСТЬNULL(Итог.Итог10, 0)) КАК Итог10,
	               |	СУММА(ЕСТЬNULL(Итог.Итог12, 0)) КАК Итог12,
	               |	СУММА(ЕСТЬNULL(Итог.Итог7, 0)) КАК Итог7,
	               |	СУММА(ЕСТЬNULL(Итог.Итог9, 0)) КАК Итог9,
	               |	СУММА(ЕСТЬNULL(Итог.Итог11, 0)) КАК Итог11,
	               |	СУММА(ЕСТЬNULL(Итог.Итог13, 0)) КАК Итог13
	               |ИЗ
	               |	Итог КАК Итог
	               |
	               |СГРУППИРОВАТЬ ПО
	               |	Итог.Организация,
	               |	Итог.Услуга";
	
	Запрос.УстановитьПараметр("НачалоПериода", Период.ДатаНачала);
	Запрос.УстановитьПараметр("КонецПериода",  Период.ДатаОкончания);
	//Если в движении в ХР (п.4.1) Субконто-3  Реализация датирована не текущим годом заданным в отборе отчета (предыдущие года), 
	//то сумму по номенклатуре включаем  в колонку Всего (по этому виду номенклатуры/Группы контрагента), и отдельно выносим сумму
	//в колонку < в том числе оплата задолженности за предыдущие годы>.
	ТекГодНачало = НачалоГода(ТекущаяДатаСеанса());
	Запрос.УстановитьПараметр("C", Дата("01.01.0001 0:00:00"));
	Запрос.УстановитьПараметр("По", ТекГодНачало);
	
	Запрос.УстановитьПараметр("С90021", ПланыСчетов.Хозрасчетный.СебестоимостьПродажНеЕНВД); //90.02.1
	Запрос.УстановитьПараметр("Организация", Организация);
	
	
	РезультатЗапроса = Запрос.Выполнить();
	
	Выборка = РезультатЗапроса.Выбрать();
	
	ОбластьШапки    = Макет.ПолучитьОбласть("Шапка");
	ОбластьВсего    = Макет.ПолучитьОбласть("Всего");
	ОбластьСтрока   = Макет.ПолучитьОбласть("Строка");
	
	ОбластьШапки.Параметры.Организация  = Организация; 	
	ОбластьШапки.Параметры.Начало  = Формат(Период.ДатаНачала, "ДФ=dd.MM.yyyy"); 
	ОбластьШапки.Параметры.Конец   = Формат(Период.ДатаОкончания, "ДФ=dd.MM.yyyy"); 
	ТабличныйДокумент.Вывести(ОбластьШапки);
	
	ТабличныйДокумент.Вывести(ОбластьВсего);
	
	Номер  = 1;
	Итог6 = 0;
	Итог7 = 0;
	Итог8 = 0;
	Итог9 = 0;
	Итог10 = 0;
	Итог11 = 0;
	Итог12 = 0;
	Итог13 = 0;
	Пока Выборка.Следующий() Цикл
			
		ОбластьСтрока.Параметры.Услуга  = Выборка.Услуга; 	
		ОбластьСтрока.Параметры.Номер   = Номер; 
		ОбластьСтрока.Параметры.Итог6   = Выборка.Итог6; 
		Итог6 = Итог6 + Выборка.Итог6; 
		ОбластьСтрока.Параметры.Итог7   = Выборка.Итог7;  
		Итог7 = Итог7 + Выборка.Итог7;
		ОбластьСтрока.Параметры.Итог8   = Выборка.Итог8; 
		Итог8 = Итог8 + Выборка.Итог8;
		ОбластьСтрока.Параметры.Итог9   = Выборка.Итог9;
		Итог9 = Итог9 + Выборка.Итог9;
		ОбластьСтрока.Параметры.Итог10  = Выборка.Итог10;
		Итог10 = Итог10 + Выборка.Итог10;
		ОбластьСтрока.Параметры.Итог11  = Выборка.Итог11; 
		Итог11 = Итог11 + Выборка.Итог11;
		ОбластьСтрока.Параметры.Итог12  = Выборка.Итог12; 
		Итог12 = Итог12 + Выборка.Итог12;
		ОбластьСтрока.Параметры.Итог13  = Выборка.Итог13; 
		Итог13 = Итог13 + Выборка.Итог13;;
		ТабличныйДокумент.Вывести(ОбластьСтрока);
		Номер = Номер + 1;
		
	КонецЦикла;
	
	ТабличныйДокумент.Область("R11C4").Текст = Итог6;
	ТабличныйДокумент.Область("R11C5").Текст = Итог7;
	ТабличныйДокумент.Область("R11C6").Текст = Итог8;
	ТабличныйДокумент.Область("R11C7").Текст = Итог9;
	ТабличныйДокумент.Область("R11C8").Текст = Итог10;
	ТабличныйДокумент.Область("R11C9").Текст = Итог11;
	ТабличныйДокумент.Область("R11C10").Текст = Итог12;
	ТабличныйДокумент.Область("R11C11").Текст = Итог13;
	
	ТабличныйДокумент.ВывестиГоризонтальныйРазделительСтраниц();
	ТабличныйДокумент.АвтоМасштаб = Истина;
	
	Возврат ТабличныйДокумент;
	
КонецФункции